
<html>
<head>
	<title></title>
</head>
<body>

<h2>Create a To Do List</h2>
	 <%= form_for :todos do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>

</body>
</html>	